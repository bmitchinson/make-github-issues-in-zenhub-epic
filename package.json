        "env-var": "^7.1.1",
        "eslint": "^7.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^26.0.1",
        "onchange": "^7.0.2",
        "prettier": "^2.0.5",
